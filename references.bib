	journal = {Travel Behaviour and Society},
	author = {Yue, Yang and Lan, Tian and Yeh, Anthony G. O. and Li, Qing-Quan},
	month = may,
	year = {2014},
	keywords = {Review},
	pages = {69--78},
	file = {ScienceDirect Full Text PDF:/Users/haydenatchley/Zotero/storage/ADBRDRCF/Yue et al. - 2014 - Zooming into individuals to understand the collect.pdf:application/pdf;ScienceDirect Snapshot:/Users/haydenatchley/Zotero/storage/UQNPSKPU/S2214367X13000215.html:text/html},
