@article{bills_activity-based_2012,
	title = {Activity-{Based} {Travel} {Models} and {Transportation} {Equity} {Analysis}: {Research} {Directions} and {Exploration} of {Model} {Performance}},
	volume = {2320},
	issn = {0361-1981},
	shorttitle = {Activity-{Based} {Travel} {Models} and {Transportation} {Equity} {Analysis}},
	url = {https://doi.org/10.3141/2320-03},
	doi = {10.3141/2320-03},
	abstract = {The current state of practice for transportation equity analysis is moving toward the use of activity-based travel models for scenario analysis. However, little has been done to validate the use of these models for equity analysis. The first objective of this paper is to present a research framework for the equity analysis of long-range transportation plans, for the purpose of critiquing the current state of practice and pointing to key research needs. This research framework is used to identify four research areas for furthering transportation equity analysis: (a) identifying transportation priorities for different groups of interest, (b) identifying appropriate modeling tools for measuring equity outcomes, (c) identifying and analyzing equity indicators, and (d) linking the equity indicators and the transportation priorities. The second objective is to explore how well an activity-based travel demand model represents heterogeneity of travel behavior among different income classes, for the purpose of transportation equity analysis. Graphical and statistical tools are used to access how well a sample of the model data compares with a sample of travel survey data for the San Francisco Bay Area in California. The results show that although the tests of distribution equality fail, the general shape of the distributions, the central tendencies, and the relative difference of the low- versus high-income samples perform well.},
	language = {en},
	number = {1},
	urldate = {2023-04-10},
	journal = {Transportation Research Record},
	author = {Bills, Tierra S. and Sall, Elizabeth A. and Walker, Joan L.},
	month = jan,
	year = {2012},
	note = {Publisher: SAGE Publications Inc},
	keywords = {Equity},
	pages = {18--27},
	annote = {ABM is disaggregate data allowing for indiv level analysis. this is useful for equity purposes
qualitative approaches can work irrespective of model type, but the specified quantitative analysis can only be done if there is data on the individual (as in a synthetic population)

},
	annote = {Annotations(4/20/2023, 3:07:17 PM)
“These models are desirable for equity analysis, given that the model data are disaggregate, allowing for individual level equity indicators to be calculated and distributions of these indicators to be explored.” (Bills et al., 2012, p. 18)“TransporTaTion EquiTy analysis in rEgional planning” (Bills et al., 2012, p. 18)“A common qualitative approach is to map the residential locations of minority and low-income communities in relation to the location of the proposed transportation improvement(s) and discern whether these communities are being overly exposed to transportation externalities (air or noise pollution, traffic congestion, etc.) (4–6). A predominant quantitative approach is described in the following three steps: 1. Divide the population into groups; for example, people and communities of concern can be classified by ethnicity, income level, education, and so forth. 2. Calculate equity indicators (e.g., average travel times, transit mode share, accessibility to jobs by mode) for the different groups and scenarios. 3. Compare the average or percentage changes in these measured values between the groups and scenarios (simulating changes after some transportation policy or project has been implemented).” (Bills et al., 2012, p. 19)“Several studies have used travel demand models as a platform for assessing the equity impacts of transportation projects and policies. That is, the travel demand model is used to simulate the changes in travel behavior that result from a particular transportation project or policy, and then the predominant equity analysis method described earlier is implemented. These examples include the use of aggregate four-step travel models (4, 5, 7), microsimulation travel demand models (8), activity-based travel demand models (9, 10), and integrated transportation–land use and macroeconomic models (6, 11).” (Bills et al., 2012, p. 19)(Bills et al., 2012, p. 19)“Still, there is a need to assess whether the model output represents the travel behavior differences that are important for equity analysis, including the differences between the travel behavior of high- and low-income residents. In practice, the significance of sociodemographic variables and the calibration of model parameters to match trip- and household-level characteristics are seen as sufficient for sensitivity purposes. However, for activity-based travel models, which allow for travel data to be explored at the person level, no studies were found that have sought to validate whether the model captures person-level differences between interest groups. It is also important to note that when dealing with the travel models, which are estimated and calibrated on a sample of data (on individuals and their travel patterns), this sample of data is also in question. The ability of the travel model to be sensitive to the differences between interest groups is tied to how well the sample data reflect the true characteristics of these groups. If the field is to make progress in equity analysis as an application of activity-based travel modeling, these questions must be addressed.” (Bills et al., 2012, p. 20)“This section focuses on Research Area 2 and explores the model specification and performance of a San Francisco Bay Area activity-based model (SF-CHAMP, Version 4.1, Harold) (34), for its representation of the heterogeneity of travel behavior across income groups” (Bills et al., 2012, p. 21)“Because income is a variable in the model that is both relevant and commonly used for equity analysis, this assessment uses income classes as interest groups.” (Bills et al., 2012, p. 21)“The use of the distributions of travel time indicators, by income class, builds on a common equity indicator currently used by practitioners: the change in average travel time by income class. By using the disaggregate data generated from activity-based travel models, the potential to assess distributions of travel times (or the distribution of the change in travel times for individuals) can be demonstrated by implementing a transportation plan. Additionally, there is the potential to explore how well the travel model represents the observed heterogeneity among the travel behavior of different communities of concern. The authors, however, are not suggesting that distribution shifts in travel time are the most desirable transportation equity indicator. There are a number of cases in which using such an indicator could be misleading, but it is used as an example here for its simplicity.” (Bills et al., 2012, p. 21)“This analysis points to activity-based travel models as being a reasonable tool for assessing the differences across income classes, although there are some statistical differences between the data generated from the travel model and the observed data.” (Bills et al., 2012, p. 26)
},
	file = {Bills et al. - 2012 - Activity-Based Travel Models and Transportation Eq.pdf:/Users/haydenatchley/Zotero/storage/MXTQ7F66/Bills et al. - 2012 - Activity-Based Travel Models and Transportation Eq.pdf:application/pdf},
}

@article{zorn_incorporating_2012,
	title = {Incorporating crowding into the {San} {Francisco} activity-based travel model},
	volume = {39},
	issn = {1572-9435},
	url = {https://doi.org/10.1007/s11116-012-9405-x},
	doi = {10.1007/s11116-012-9405-x},
	abstract = {Information produced by travel demand models plays a large role decision making in many metropolitan areas, and San Francisco is no exception. Being a transit first city, one of the most common uses for San Francisco’s travel model SF-CHAMP is to analyze transit demand under various circumstances. SF-CHAMP v 4.1 (Harold) is able to capture the effects of several aspects of transit provision including headways, stop placement, and travel time. However, unlike how auto level of service in a user equilibrium traffic assignment is responsive to roadway capacity, SF-CHAMP Harold is unable to capture any benefit related to capacity expansion, crowding’s effect on travel time nor or any of the real-life true capacity limitations. The failure to represent these elements of transit travel has led to significant discrepancies between model estimates and actual ridership. Additionally it does not allow decision-makers to test the effects of policies or investments that increase the capacity of a given transit service. This paper presents the framework adopted into a more recent version of SF-CHAMP (Fury) to represent transit capacity and crowding within the constraints of our current modeling software.},
	language = {en},
	number = {4},
	urldate = {2023-04-17},
	journal = {Transportation},
	author = {Zorn, Lisa and Sall, Elizabeth and Wu, Daniel},
	month = jul,
	year = {2012},
	keywords = {Development},
	pages = {755--771},
	file = {Zorn et al. - 2012 - Incorporating crowding into the San Francisco acti.pdf:/Users/haydenatchley/Zotero/storage/ULE8ZTC3/Zorn et al. - 2012 - Incorporating crowding into the San Francisco acti.pdf:application/pdf},
}

@article{park_calibration_2011,
