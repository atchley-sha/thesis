	keywords = {CITED, Built environment, Gravity model, Intrazonal trips, Multilevel modeling, Trip distribution},
	pages = {2087--2108},
	file = {Full Text PDF:/home/shayd/Zotero/storage/GB9JV8K5/Park et al. - 2020 - Intrazonal or interzonal Improving intrazonal tra.pdf:application/pdf},
