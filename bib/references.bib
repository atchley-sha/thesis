@phdthesis{bramberga_teleworking_2023,
	title = {Teleworking in {Four}-{Step} {Transport} {Demand} {Models}},
	abstract = {The opportunity of teleworking has impacted people travel habits, especially work-related trips. In most
cases the existing transport demand models lack the consideration of teleworking. This thesis
investigates the characteristics of teleworkers based on earlier studies about teleworking and is aiming
to improve the understanding of those characteristics to determine the living and work locations of
teleworkers. The result of this thesis is a simple approach of teleworking probability for trip generation
in transport demand models. The method is then applied on a case study of Uppsala municipality. The
main findings suggest that it is important to consider both ability and willingness to telework in transport
demand models. One of the challenges in modelling teleworking, is to find data on the most basic factors
that affect the probability. In addition, an important conclusion is that more knowledge is needed about
the weight of each factor on the probability of telework.},
	school = {KTH Royal Institute of Technology},
	author = {Bramberga, Katrina Aleksandra},
	month = jun,
	year = {2023},
	annote = {There is no single approach to modeling telecommute. It should be case by case. 
},
	file = {Bramberga - 2023 - Teleworking in Four-Step Transport Demand Models.pdf:/Users/kamrynmansfield/Zotero/storage/3XJTRLWB/Bramberga - 2023 - Teleworking in Four-Step Transport Demand Models.pdf:application/pdf},
}